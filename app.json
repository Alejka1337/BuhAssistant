{
  "expo": {
    "name": "eGlavBuh",
    "slug": "buhassistant",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "locales": {
      "uk": "./locales/uk.json"
    },
    "primaryLanguage": "uk",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#228822"
    },
    "jsEngine": "hermes",
    "ios": {
      "buildNumber": "10",
      "supportsTablet": true,
      "bundleIdentifier": "com.alejka1337.eglavbuh.dev",
      "scheme": "com.googleusercontent.apps.914514821616-47musasu3ster3fjvjlbehc8fdrdgbno",
      "config": {
        "usesApns": true
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "remote-notification"
        ],
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "notification": {
      "icon": "./assets/images/icon.png",
      "color": "#228822",
      "iosDisplayInForeground": true,
      "androidMode": "default",
      "androidCollapsedTitle": "eGlavBuh"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.buhassistant"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "backgroundColor": "#1a1d21",
      "themeColor": "#228822",
      "lang": "uk",
      "name": "eGlavBuh",
      "shortName": "eGlavBuh",
      "description": "Бухгалтерський помічник для ФОП та юридичних осіб"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/icon.png",
          "color": "#228822",
          "sounds": []
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8698ae71-7811-4098-ab40-e39b6dcffcf4"
      },
      "googleWebClientId": "914514821616-rh81j21a2qbqu104j45j6j09661jo6qm.apps.googleusercontent.com",
      "googleIosClientId": "914514821616-47musasu3ster3fjvjlbehc8fdrdgbno.apps.googleusercontent.com",
      "apiUrl": "https://eglavbuh.com.ua"
    }
  }
}
